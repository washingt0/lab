{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-httpsecure": {
      "allowed_hosts": [],
      "ssl_proxy_headers": {},
      "browser_xss_filter": true,
      "ssl_redirect": true,
      "frame_deny": true,
      "custom_frame_options_value": "DENY"
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "lab",
  "port": 443,
  "endpoints": [
    {
      "endpoint": "/iam/auth/login",
      "method": "POST",
      "output_encoding": "no-op",
      "extra_config": {
        "github.com/devopsfaith/krakend-httpsecure": {
          "allowed_hosts": [],
          "ssl_proxy_headers": {},
          "browser_xss_filter": true,
          "ssl_redirect": true,
          "frame_deny": true,
          "custom_frame_options_value": "DENY"
        }
      },
      "headers_to_pass": [
        "User-Agent",
        "Content-Type",
        "X-Forwarded-For"
      ],
      "backend": [
        {
          "url_pattern": "/v1/auth/login",
          "encoding": "no-op",
          "sd": "static",
          "method": "POST",
          "extra_config": {},
          "host": [
            "http://iam:9000"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/iam/auth/session",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "github.com/devopsfaith/krakend-httpsecure": {
          "allowed_hosts": [],
          "ssl_proxy_headers": {},
          "browser_xss_filter": true,
          "ssl_redirect": true,
          "frame_deny": true,
          "custom_frame_options_value": "DENY"
        },
        "github.com/devopsfaith/krakend-jose/validator": {
          "alg": "RS256",
          "cache": true,
          "disable_jwk_security": true,
          "issuer": "lab/iam",
          "jwk-url": "http://iam:9090/keys",
          "audience": [
            "https://lab.washingt0.me"
          ]
        }
      },
      "headers_to_pass": [
        "User-Agent",
        "Content-Type",
        "X-Forwarded-For",
        "Authorization"
      ],
      "backend": [
        {
          "url_pattern": "/v1/auth/session",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "extra_config": {},
          "host": [
            "http://iam:9000"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ],
  "tls": {
    "public_key": "/private/lab/cert.pem",
    "private_key": "/private/lab/key.pem"
  }
}